{
  "openai": {
    "apiKey": "",
    "model": "gpt-4",
    "baseURL": "https://api.openai.com/v1",
    "temperature": 0.6,
    "maxTokens": 1000,
    "streaming": true,
    "timeout": 30000
  },
  "langgraph": {
    "maxRetries": 3,
    "timeout": 30000,
    "maxConcurrency": 5,
    "enableStreaming": true,
    "logLevel": "debug",
    "enablePerformanceMonitoring": true
  },
  "logging": {
    "level": "debug",
    "file": "waidrin-dev.log",
    "enableConsole": true,
    "enableFile": true,
    "maxFileSize": "10m",
    "maxFiles": 5
  },
  "database": {
    "url": "",
    "maxConnections": 10,
    "timeout": 5000
  },
  "app": {
    "name": "Waidrin LangGraph Engine",
    "version": "1.0.0",
    "environment": "development",
    "port": 3000,
    "host": "localhost"
  },
  "rateLimit": {
    "windowMs": 60000,
    "maxRequests": 100,
    "skipSuccessfulRequests": false
  },
  "cache": {
    "ttl": 300,
    "maxSize": 1000,
    "enableMemoryCache": true
  }
}
