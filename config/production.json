{
  "openai": {
    "apiKey": "",
    "model": "gpt-4",
    "baseURL": "https://api.openai.com/v1",
    "temperature": 0.6,
    "maxTokens": 1000,
    "streaming": true,
    "timeout": 30000
  },
  "langgraph": {
    "maxRetries": 3,
    "timeout": 30000,
    "maxConcurrency": 10,
    "enableStreaming": true,
    "logLevel": "info",
    "enablePerformanceMonitoring": true
  },
  "logging": {
    "level": "info",
    "file": "waidrin.log",
    "enableConsole": false,
    "enableFile": true,
    "maxFileSize": "50m",
    "maxFiles": 10
  },
  "database": {
    "url": "",
    "maxConnections": 20,
    "timeout": 5000
  },
  "app": {
    "name": "Waidrin LangGraph Engine",
    "version": "1.0.0",
    "environment": "production",
    "port": 3000,
    "host": "0.0.0.0"
  },
  "rateLimit": {
    "windowMs": 60000,
    "maxRequests": 1000,
    "skipSuccessfulRequests": false
  },
  "cache": {
    "ttl": 600,
    "maxSize": 5000,
    "enableMemoryCache": true
  }
}
